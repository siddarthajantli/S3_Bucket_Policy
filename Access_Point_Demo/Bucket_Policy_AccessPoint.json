{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Sid": "S3-AP-Demo",
			"Effect": "Allow",
            "Principal": {
                "AWS": "*"
            },
			"Action": "*",
			"Resource": [
                "arn:aws:s3:::terraform-state-sidd-lab",
                "arn:aws:s3:::terraform-state-sidd-lab/*"
            ],
            "Condition": {
                "StringEquals": {
                "s3:DataAccessPointAccount": "<Account_ID>"
                }
            }
		}
	]
}